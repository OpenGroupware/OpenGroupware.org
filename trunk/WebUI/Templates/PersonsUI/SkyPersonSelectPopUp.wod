// bind to class

PopUp: WOPopUpButton {
  list      = visibleCompanies;
  item      = item;
  value     = item.companyId;
  string    = itemLabel;
  selection = selectedCompany;
  onChange  = ^onChange;
}

WithSearchField: WOConditional {
  condition = ^showSearchField;
}

SearchField: WOTextField {
  value = searchPerson;
  size  = 10;
}
