xmlrpcd_TOOL_LIBS += \
	-lOGoContacts -lOGoScheduler -lOGoJobs -lOGoAccounts -lOGoProject \
	-lOGoSchedulerTools 	\
	-lOGoDocuments		\
	-lLSFoundation		\
