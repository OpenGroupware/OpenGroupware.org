	-L$(DOCAPI_DIR)/OGoProject/$(GNUSTEP_OBJ_DIR)	\

ADDITIONAL_INCLUDE_DIRS += \
	-I$(OGoROOT)/Logic      \
	-I$(OGoROOT)/DocumentAPI

ADDITIONAL_LIB_DIRS += -L/usr/local/lib -L/usr/lib
