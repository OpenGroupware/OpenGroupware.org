Image: WOImage {
  data     = object;
  mimeType = contentType.stringValue;
  ALT      = fileName;
  BORDER   = 0;
}
ImageURI: WOImage {
  src      = uri;
  ALT      = fileName;
  BORDER   = 0;
}

UseURI: WOConditional {
  condition = useURI;
}
UseData: WOConditional {
  condition = useURI;
  negate    = YES;
}
