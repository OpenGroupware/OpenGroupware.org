// bind template to controller

Phrase: WOString {
  value      = localPhrase;
  escapeHTML = NO;
}

Font: WOGenericContainer {
  elementName = "font";
  color       = "red";
  size        = config.editFont_size;
  face        = config.font_face;
}

Buttons: SkyButtonRow {
  ordering = ( ok, cancel );
  onOk     = ok;
  ok       = labels.ok;
  onCancel = cancel;
  cancel   = labels.cancel;
}
